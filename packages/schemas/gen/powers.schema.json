{
  "$ref": "#/definitions/DMSpecialPowers",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "DMSpecialPowers": {
      "additionalProperties": {
        "anyOf": [
          {
            "$ref": "#/definitions/DmSpecialPower"
          },
          {
            "$ref": "#/definitions/DmSummonPower"
          }
        ]
      },
      "description": "A map from entityDefName to the values inside that entitydef. The name will appear in `entityDef \"<NAME_HERE>\". The value is used to generate the body of the entityDef.",
      "type": "object"
    },
    "DmSpecialPower": {
      "additionalProperties": false,
      "properties": {
        "bullrush": {
          "examples": [
            10
          ],
          "type": "integer"
        },
        "class": {
          "const": "dmSpecialPower",
          "type": "string"
        },
        "damagemod": {
          "examples": [
            -0.9
          ],
          "type": "number"
        },
        "hitTargetEmitter": {
          "examples": [
            "axe_flurry_emitter"
          ],
          "type": "string"
        },
        "menudescription": {
          "default": "No one will ever be able to read this.",
          "type": "string"
        },
        "nummeleeattacks": {
          "type": "integer"
        },
        "powername": {
          "examples": [
            "So Many Bites!"
          ],
          "type": "string"
        },
        "script": {
          "description": "Reference to a function inside a file in code/. For example, setting \"script\" to \"modmans_mods.somanybites\" means your script lives in code/modmans_mods.cs and within the class ScriptFunctions you have a method named `somanybites`",
          "examples": [
            "modmans_mods.somanybites"
          ],
          "type": "string"
        },
        "selectRange": {
          "examples": [
            1
          ],
          "type": "integer"
        }
      },
      "required": [
        "class",
        "powername",
        "menudescription",
        "nummeleeattacks",
        "damagemod",
        "selectRange",
        "bullrush",
        "hitTargetEmitter",
        "script"
      ],
      "type": "object"
    },
