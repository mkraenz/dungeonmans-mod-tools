{
  "name": "@dungeonmans-mod-tools/cli",
  "version": "0.7.0",
  "private": false,
  "license": "MIT",
  "description": "CLI to help you develop Mod Content for Dungeonmans.",
  "keywords": [
    "dungeonmans",
    "modding",
    "game",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mkraenz/dungeonmans-mod-tools.git"
  },
  "type": "module",
  "dependencies": {
    "@commander-js/extra-typings": "^13.1.0",
    "commander": "^13.1.0",
    "lodash-es": "^4.17.21",
    "tslib": "^2.3.0"
  },
  "bin": {
    "@dungeonmans-mod-tools/cli": "dist/main.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "publishConfig": {
    "access": "public"
  }
}
