{
  "name": "@dungeonmans-modding-tools/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "json-to-yaml:watch": "tsx ./tools/json-to-yaml-watcher.ts",
    "jsonschema:gen:monster": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/actordata/monster.ts' --type 'DmMonster' > 'libs/dmans-schemas/gen/monster.schema.json'",
    "jsonschema:gen:monsters": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/actordata/monsters.ts' --type 'DmMonsters' > 'libs/dmans-schemas/gen/monsters.schema.json'",
    "jsonschema:gen:monster-sprite": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/spritedata/monster-sprite.ts' --type 'DmMonsterSprite' > 'libs/dmans-schemas/gen/monster-sprite.schema.json'",
    "jsonschema:gen:monster-sprites": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/spritedata/monster-sprites.ts' --type 'DmMonsterSprites' > 'libs/dmans-schemas/gen/monster-sprites.schema.json'",
    "jsonschema:gen:item-sprite": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/spritedata/item-sprite.ts' --type 'DmItemSprite' > 'libs/dmans-schemas/gen/item-sprite.schema.json'",
    "jsonschema:gen:item-sprites": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/spritedata/item-sprites.ts' --type 'DmItemSprites' > 'libs/dmans-schemas/gen/item-sprites.schema.json'",
    "jsonschema:gen:encounter": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/encounterdata/encounter.ts' --type 'DmEncounter' > 'libs/dmans-schemas/gen/encounter.schema.json'",
    "jsonschema:gen:encounters": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/encounterdata/encounters.ts' --type 'DmEncounters' > 'libs/dmans-schemas/gen/encounters.schema.json'",
    "jsonschema:gen:encounter-table": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/tabledata/encounter-table.ts' --type 'DmEncounterTable' > 'libs/dmans-schemas/gen/encounter-table.schema.json'",
    "jsonschema:gen:encounter-tables": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/tabledata/encounter-tables.ts' --type 'DmEncounterTables' > 'libs/dmans-schemas/gen/encounter-tables.schema.json'",
    "jsonschema:gen:plot": "ts-json-schema-generator --path 'libs/dmans-schemas/src/lib/plotdata/plot-threads.ts' --type 'DmPlot' > 'libs/dmans-schemas/gen/plot.schema.json'",
    "init:mod": "tsx tools/init-mod-dir.ts",
    "build:mod": "tsx tools/build-mod.ts",
    "init:mod:testmodname": "tsx tools/init-mod-dir.ts",
    "build:mod:testmodname": "tsx tools/build-mod.ts mods/testmodname/src mods/testmodname/dist"
  },
  "private": true,
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nx/eslint": "20.3.2",
    "@nx/eslint-plugin": "20.3.2",
    "@nx/jest": "20.3.2",
    "@nx/js": "20.3.2",
    "@nx/node": "20.3.2",
    "@nx/rspack": "20.3.2",
    "@rspack/cli": "^1.1.5",
    "@rspack/core": "^1.1.5",
    "@rspack/dev-server": "1.0.9",
    "@rspack/plugin-minify": "^0.7.5",
    "@rspack/plugin-react-refresh": "^1.0.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.5.12",
    "@types/node": "22.10.10",
    "chokidar": "^4.0.3",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "js-yaml": "^4.1.0",
    "nx": "20.3.2",
    "prettier": "^2.6.2",
    "react-refresh": "~0.14.0",
    "ts-jest": "^29.1.0",
    "ts-json-schema-generator": "^2.3.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "tsx": "^4.19.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.13.0"
  }
}
